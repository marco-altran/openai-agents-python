{
    "name": "openai/agents",
    "description": "OpenAI Agents SDK for PHP",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.7",
        "opis/json-schema": "^2.3",
        "react/promise": "^3.0",
        "react/event-loop": "^1.4",
        "psr/http-client": "^1.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "OpenAI\\Agents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAI\\Agents\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}